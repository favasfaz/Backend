            <div class="container-xxl flex-grow-1 container-p-y">
             
              <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light"></span>Add Product</h4>
             {{#if alert}}
              <div class="alert alert-warning alert-dismissible fade show" role="alert">
   {{alert}}
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
{{/if}}
              <div class="row">
               
             

                <!-- Form controls -->
   <form  action="/addProduct" method="post" enctype="multipart/form-data">
              <div><p style="color: red;">{{err}}</p></div>
  <div class="row mb-4">

    <div class="col">
      <div class="form-outline">
                <label class="form-label"  for="form6Example1">Product Name</label>
        <input type="text" id="form6Example1" name="productName"  class="form-control" />
      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">Price</label>
        <input type="text" id="form6Example2" name="price"  class="form-control" />
      </div>
    </div>
  </div>
    <div class="row mb-4">

    <div class="col">
      <div class="form-outline">
                <label class="form-label"  for="form6Example1">Stock</label>
        <input type="text" id="form6Example1" name="stock"  class="form-control" />
      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">Description</label>
        <input type="text" id="form6Example2" name="description"  class="form-control" />
      </div>
    </div>
  </div>
    <div class="row mb-4">

    <div class="col">
     <div class="form-group">
                                              <label for="exampleSelectGender1">category</label>
                        <select name="category" class="form-control" id="exampleSelectGender1" required>
                          {{#each data.categories}}
                          <option >{{this.name}}</option>
                          {{/each}}
                        </select>
                      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">OS</label>
        <input type="text" id="form6Example2" name="os"  class="form-control" />
      </div>
    </div>
  </div>
    <div class="row mb-4">

    <div class="col">
      <div class="form-group">
                                              <label for="exampleSelectGender1">brand</label>
                        <select name="brand" class="form-control" id="exampleSelectGender1" required>
                          {{#each allBrands.allBrands}}
                          <option >{{this.name}}</option>
                          {{/each}}
                        </select>
                      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">Shipping cost</label>
        <input type="text" id="form6Example2" name="shippingCost"  class="form-control" />
      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">color</label>
        <input type="text" id="form6Example2" name="color"  class="form-control" />
      </div>
    </div>
   
  </div>

   <div class="row mb-4">

    <div class="col">
      <div class="form-outline">
                <label class="form-label"  for="form6Example1">Processor</label>
        <input type="text" id="form6Example1" name="processor"  class="form-control" />
      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">Memory</label>
        <input type="text" id="form6Example2" name="memory"  class="form-control" />
      </div>
    </div>
    <div class="col">
      
    </div>
   
   
  </div>
  <div class="row mb-4">

    <div class="col">
      <div class="form-outline">
                <label class="form-label"  for="form6Example1">Discount</label>
        <input type="text" id="form6Example1" name="discount"  class="form-control" />
      </div>
    </div>
    <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">IMAGE1</label>
        <input type="file" id="form6Example2" name="images"  class="form-control" required multiple />
      </div>
    </div>
     <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">IMAGE2</label>
        <input type="file" id="form6Example2" name="images1"  class="form-control" required multiple />
      </div>
    </div>
      <div class="col">
      <div class="form-outline">
                <label class="form-label" for="form6Example2">IMAGE3</label>
        <input type="file" id="form6Example2" name="images2"  class="form-control" required multiple />
      </div>
    </div>
   
  </div>

  <!-- Submit button -->
  <button type="submit" class="btn btn-primary btn-block mb-4">ADD PRODUCT</button>
