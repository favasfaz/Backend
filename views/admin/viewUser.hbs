  <body>
            {{>admin-Dashbord}}

    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
      <div class="layout-container">
        <!-- Menu -->
      
        <!-- Layout container -->
        <div class="layout-page">
          <!-- Navbar -->

        

          <!-- Content wrapper -->
          <div class="content-wrapper">
            <!-- Content -->

            <div class="container-xxl flex-grow-1 container-p-y">

               
             

                <!-- Form controls -->
					 <div class="container">
					 
				     <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light"></span> View Users</h4>
					 <h4>totolUsers:{{count}}</h4>
<div class="row">
	<div class="col-lg-12">
		<div class="main-box clearfix">
			<div class="table-responsive">
				<table class="table user-list">
					<thead>
						<tr>
							<th><span>User</span></th>
							<th><span>Created</span></th>
							<th class="text-center"><span>LastName</span></th>
							<th><span>Email</span></th>
								<th><span>Status</span></th>
							<th>&nbsp;</th>
						</tr>
					</thead>
					<tbody>
                        {{#each user}}
						<tr>
							<td>
								<img style="width: 70px ;" src="https://bootdey.com/img/Content/avatar/avatar1.png" alt="">
								<a href="#" class="user-link">{{this.fName}}</a>
								<span class="user-subhead"></span>
							</td>
							<td>
								{{this.create}}
								</td>
							<td class="text-center">
								<span class="label label-default">{{this.lName}}</span>
							</td>
							
							<td>
								<a href="#">{{this.email}}</a>
							</td>
							<td>
								<a href="#">{{this.status}}</a>
							</td>
							 <td style="width: 20%;">
						
								{{#if status}}
								<a id="unblock"  style="margin-left:10px ;" href="/blockUser/{{this.email}}" class="table-link">
									<span class="fa-stack">
										<i  class="fa-solid fa-block">Block</i>
										
									</span>
								</a>
								{{else}}
								<a id="block" style="margin-left:10px ;" href="/unBlockUser/{{this.email}}" class="table-link">
									<span class="fa-stack">
										<i  class="fa-solid fa-block">unBlock</i>
										
									</span>
								</a>
								{{/if}}
								<a href="/deleteUser/{{this.email}}"  style="margin-left:20px ;" onclick="return confirm('Are you sure you want to delete this item?')" class="table-link danger">
									<span class="fa-stack">
										<i class="fa fa-square fa-stack-2x"></i>
										<i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
									</span>
								</a>
							</td>
						</tr>
					{{/each}}
					</tbody>
				</table>
			</div>
			<ul class="pagination pull-right">
				<li><a href="#"><i class="fa fa-chevron-left"></i></a></li>
				<li><a href="#">1</a></li>
				<li><a href="#">2</a></li>
				<li><a href="#">3</a></li>
				<li><a href="#">4</a></li>
				<li><a href="#">5</a></li>
				<li><a href="#"><i class="fa fa-chevron-right"></i></a></li>
			</ul>
		</div>
	</div>
</div>
</div>


               
               
               
              

